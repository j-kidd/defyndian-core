CREATE DEFINER=`homeauto`@`%` PROCEDURE `getDevicesInRoom`(IN roomName VARCHAR(45))
BEGIN
IF roomName = "" then
	SELECT d.deviceID as id, d.deviceName as name, r.roomName as room 
    FROM lightwave.devices d, lightwave.rooms r
    WHERE d.roomid=r.id;
else
	SELECT d.deviceID as id, d.deviceName as name, r.roomName as room
    FROM lightwave.devices d, lightwave.rooms r
    WHERE d.roomid=r.id AND r.roomName=roomName;
end if;
END ;;
